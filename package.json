{
	"name": "@nooma-tech/n8n-nodes-aws-sqs",
	"version": "1.0.2",
	"description": "n8n nodes for AWS SQS integration",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"nodes",
		"aws",
		"sqs",
		"queue",
		"messaging"
	],
	"private": false,
	"license": "MIT",
	"author": {
		"name": "Nooma Tech",
		"email": "contato@nooma.tech"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Nooma-Tech/n8n-nodes-aws-sqs.git"
	},
	"engines": {
		"node": ">=20"
	},
	"main": "index.js",
	"scripts": {
		"build": "npx rimraf dist && tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes --write",
		"lint": "eslint nodes package.json",
		"lintfix": "eslint nodes package.json --fix",
		"test": "jest",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [],
		"nodes": [
			"dist/nodes/Aws/SQS/AwsSqsTrigger.node.js"
		]
	},
	"dependencies": {
		"@aws-sdk/client-sqs": "^3.705.0"
	},
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/node": "^24.3.0",
		"@typescript-eslint/parser": "~8.43.0",
		"eslint": "^8.57.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.3",
		"gulp": "^5.0.1",
		"gulp-rename": "^2.0.0",
		"gulp-svgmin": "^4.1.0",
		"jest": "^30.1.1",
		"prettier": "^3.0.0",
		"ts-jest": "^29.1.0",
		"typescript": "^5.8.2",
		"n8n-workflow": "*"
	},
	"overrides": {
		"form-data": "^4.0.4"
	},
	"publishConfig": {
		"access": "public"
	}
}
